
@import "../../../style/swiper.wxss";
.contain{
   background: #f3eff1;
}

.type_cont {
  margin-top: 5px;
 
}
.type_flex{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  background: white;
  padding: 10px;
  justify-content: space-between;
}
.type_for {
  width: 150rpx;
  height: 105px;
  text-align: center;
}

.type_for image {
  width: 100%;
  height: 75px;
}
.type_cont_text {
  font-size: 15px;
  padding-top: 5px;
}
.recommond_nav {
  margin-top: 7px;
  background: white;
  padding: 10px;
  font-size: 18px;
}

.recommond_cont{
  background: white;
}
.recommond_flex {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  flex-wrap: wrap;

}
.recommond_for {
  width:44%;
  height: 200rpx;
}
.recommond_for:nth-child(3){
  padding-top: 60px;
  padding-bottom: 40px;
}
.recommond_for:nth-child(4){
  padding-top: 60px;
  padding-bottom: 40px;
}

.recommond_for>image {
  width: 100%;
  height: 100%;
}
.recommond_name {
 display: flex;
 flex-direction: row;
 justify-content: space-between;
}

.recommond_right {
  width: 80rpx;
  height: 30rpx;
}
.recommond_right image{
  width: 100%;
  height: 100%;
}
.recommond_left{
  font-size: 15px;
}
